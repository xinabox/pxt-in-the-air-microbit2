{
    "name": "in-the-air-microbit2",
    "version": "0.1.13",
    "description": "Extension for in-the-air-microbt-2 kit",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "itr.ts"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "private_addressing": 0,
                    "advertising_timeout": 0,
                    "tx_power": 0,
                    "dfu_service": 0,
                    "event_service": 0,
                    "device_info_service": 0,
                    "eddystone_url": 0,
                    "eddystone_uid": 0,
                    "open": 0,
                    "pairing_mode": 0,
                    "whitelist": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_NO_MITM",
                    "partial_flashing": 1,
                    "enabled": 0
                }
            }
        }
    }
}
